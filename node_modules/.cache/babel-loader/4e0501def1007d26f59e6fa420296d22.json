>\n    <div class=\"container\">\n      <div class=\"row\" v-if=\"posts.length\">\n        <div class=\"col-md-6 col-lg-4\" v-for=\"post in posts\" :key=\"post.id\">\n            <div class=\"post-item\">\n              <div class=\"thumb\">\n                <a href=\"blog-details.html\"><img :src=\"post.post_thumb_src\" alt=\"Roomedia-Blog\"></a>\n              </div>\n              <div class=\"content\">\n                <h4 class=\"title\">\n                  <a href=\"blog-details.html\">{{ post.title.rendered }}</a>\n                </h4>\n                <p>{{ post.custom_excerpt }}</p>\n                {{ firstCat(post) }}\n                <div class=\"meta bottom meta-bottom\">\n                  <div>\n                    <a class=\"category\" href=\"blog.html\"><img class=\"icon-img\" src=\"assets/img/icons/layer.png\" alt=\"Icon\"> In <span v-for=\"category in post.post_categories\" :key=\"category.term_id\">{{ category.name }}</span></a>\n                    <a class=\"post-date\" href=\"blog.html\"><img class=\"icon-img\" src=\"assets/img/icons/calender.png\" alt=\"Icon\"> 16/4/2021</a>\n                  </div>\n                  <div class=\"post-share\">\n                    <a class=\"icon-share\" href=\"#/\"><img class=\"icon-img\" src=\"assets/img/icons/share.png\" alt=\"Icon\"></a>\n                    <ul>\n                      <li><a class=\"color-facebook\" href=\"#/\"><i class=\"social_facebook\"></i></a></li>\n                      <li><a class=\"color-twitter\" href=\"#/\"><i class=\"social_twitter\"></i></a></li>\n                      <li><a class=\"color-dribbble\" href=\"#/\"><i class=\"social_dribbble\"></i></a></li>\n                      <li><a class=\"color-pinterest\" href=\"#/\"><i class=\"social_pinterest\"></i></a></li>\n                    </ul>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n      </div>\n      <div class=\"row\" v-else>\n        <div class=\"loader\">\n          <div class=\"spinner-grow text-warning\" style=\"width: 3rem; height: 3rem;\" role=\"status\">\n            <span class=\"sr-only\"></span>\n          </div>\n        </div>\n      </div>\n    </div>\n  </section>\n</template>\n\n<script>\nimport axios from 'axios';\nimport BreadCrumb from '@/components/BreadCrumb.vue'\n\nexport default {\n  name: 'Home',\n  components: {\n    BreadCrumb\n  },\n  data(){\n    return{\n      posts: []\n    }\n  },\n  methods: {\n    firstCat(object) {\n      return object.post_categories[0]\n    }\n  },\n  mounted() {\n    axios\n      .get('https://works.aiarnob.com/blog-api/wp-json/wp/v2/posts?per_page=12')\n      .then((response) => {\n        this.posts = response.data;\n        // console.log(response.data)\n      })\n  }\n}\n</script>\n\n<style scoped>\n.loader {\n    text-align: center;\n    margin-bottom: 80px;\n}\n</style>"],"sourceRoot":""}]}